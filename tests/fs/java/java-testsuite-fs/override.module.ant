<project name="jpf.testsuite.fs.override" xmlns:ac="antlib:net.sf.antcontrib">
	<ac:if>
		<available file="config.properties" type="file"/>
		<ac:then>
			<property file="config.properties"/>
		</ac:then>
		<ac:else>
			<fail message="Please specify a 'config.properties' file derived from 'config.properties.template' in '${basedir}' directory."/>
		</ac:else>
	</ac:if>
</project>